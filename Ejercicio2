facultad<-c("economia","ingenieria", "arquitectura", "derecho")
licenciatura<-c("actuaria","ingenieriacivil","arquitectura","derecho")
calificaciones<-c(6,4,6,8)
class(facultad)
class(calificaciones)
class(licenciatura)
rm(facultad)
rm(licenciatura)
etiq<-c("act","ingcivil","arq","der")
pie(calificaciones,etiq,main="GrÃ¡fica Calificaciones Alumnos",sub="Fuente: UAEMex",col=c("blue","red", "green", "yellow"),radius=1,clockwis=TRUE,lty=6)
